{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "arrowParens": "avoid",
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "experimentalSortImports": {
    "groups": ["builtin", "external", ["parent", "sibling", "index"]],
    "newlinesBetween": true,
  },
}
